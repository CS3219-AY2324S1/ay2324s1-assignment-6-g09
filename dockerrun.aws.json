{
    "AWSEBDockerrunVersion": 3,
    "containerDefinitions": [
        {
            "name": "user-service-db",
            "image": "postgres:latest",
            "hostname": "user-service-db",
            "essential": true,
            "memory": 512,
            "portMappings": [
                {
                    "hostPort": 5432,
                    "containerPort": 5432
                }
            ]
        },
        {
            "name": "user-service",
            "image": "920185736846.dkr.ecr.ap-southeast-1.amazonaws.com/user-service",
            "hostname": "user-service",
            "essential": true,
            "memory": 512,
            "portMappings": [
                {
                    "hostPort": 3002,
                    "containerPort": 3002
                }
            ],
            "links": [
                "user-service-db"
            ]
        },
        {
            "name": "question-service-db",
            "image": "mongo:latest",
            "hostname": "question-service-db",
            "essential": true,
            "memory": 512,
            "portMappings": [
                {
                    "hostPort": 27017,
                    "containerPort": 27017
                }
            ]
        },
        {
            "name": "question-service",
            "image": "920185736846.dkr.ecr.ap-southeast-1.amazonaws.com/question-service",
            "hostname": "question-service",
            "essential": true,
            "memory": 512,
            "portMappings": [
                {
                    "hostPort": 3001,
                    "containerPort": 3001
                }
            ],
            "links": [
                "question-service-db"
            ]
        }
    ]
  }